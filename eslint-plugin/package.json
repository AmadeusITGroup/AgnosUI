{
	"name": "@agnos-ui/eslint-plugin",
	"version": "0.0.0",
	"private": true,
	"main": "dist/index.cjs",
	"type": "module",
	"dependencies": {
		"@typescript-eslint/utils": "^8.12.2"
	},
	"peerDependencies": {
		"typescript": "~5.5.4"
	},
	"devDependencies": {
		"@angular/core": "^18.2.5",
		"@typescript-eslint/rule-tester": "^8.12.2",
		"@vitest/coverage-v8": "^2.1.4",
		"svelte": "^5.1.4",
		"svelte-eslint-parser": "^0.43.0"
	},
	"scripts": {
		"build": "wireit",
		"check": "wireit",
		"tdd": "vitest",
		"tdd:ui": "vitest --ui",
		"test": "vitest run",
		"test:coverage": "wireit"
	},
	"wireit": {
		"build": {
			"command": "vite build",
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "tsc",
			"files": [
				"src/**",
				"tsconfig.json",
				"../tsconfig.json",
				"vite.config.ts",
				"vitest.config.ts"
			],
			"output": []
		},
		"test:coverage": {
			"command": "vitest run --coverage",
			"files": [
				"src/**",
				"test/**",
				"vitest.config.ts"
			],
			"output": [
				"coverage/**"
			]
		}
	}
}
