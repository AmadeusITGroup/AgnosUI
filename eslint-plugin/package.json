{
	"name": "@agnos-ui/eslint-plugin",
	"private": true,
	"main": "dist/index.cjs",
	"type": "module",
	"dependencies": {
		"@typescript-eslint/utils": "^7.1.1"
	},
	"peerDependencies": {
		"typescript": "*"
	},
	"devDependencies": {
		"@angular/core": "~17.2.4",
		"@typescript-eslint/rule-tester": "^7.1.1",
		"@vitest/coverage-v8": "^1.3.1",
		"svelte": "^4.2.12",
		"svelte-eslint-parser": "^0.33.1"
	},
	"scripts": {
		"build": "wireit",
		"check": "wireit",
		"tdd": "vitest",
		"tdd:ui": "vitest --ui",
		"test": "vitest run",
		"test:coverage": "wireit"
	},
	"wireit": {
		"build": {
			"command": "vite build",
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "tsc",
			"files": [
				"src/**",
				"tsconfig.json",
				"../tsconfig.json",
				"vite.config.ts",
				"vitest.config.ts"
			],
			"output": []
		},
		"test:coverage": {
			"command": "vitest run --coverage",
			"files": [
				"src/**",
				"test/**",
				"vitest.config.ts"
			],
			"output": [
				"coverage/**"
			]
		}
	}
}
