{
	"name": "@agnos-ui/eslint-plugin",
	"version": "0.7.0-next.0",
	"private": true,
	"main": "dist/index.cjs",
	"type": "module",
	"dependencies": {
		"@typescript-eslint/utils": "^8.17.0"
	},
	"peerDependencies": {
		"typescript": "~5.6.3"
	},
	"devDependencies": {
		"@angular/core": "^19.0.3",
		"@typescript-eslint/rule-tester": "^8.17.0",
		"@vitest/coverage-v8": "^2.1.8",
		"svelte": "^5.9.0",
		"svelte-eslint-parser": "^0.43.0"
	},
	"scripts": {
		"build": "wireit",
		"check": "wireit",
		"tdd": "vitest",
		"tdd:ui": "vitest --ui",
		"test": "vitest run",
		"test:coverage": "wireit"
	},
	"wireit": {
		"build": {
			"command": "vite build",
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "tsc",
			"files": [
				"src/**",
				"tsconfig.json",
				"../tsconfig.json",
				"vite.config.ts",
				"vitest.config.ts"
			],
			"output": []
		},
		"test:coverage": {
			"command": "vitest run --coverage",
			"files": [
				"src/**",
				"test/**",
				"vitest.config.ts"
			],
			"output": [
				"coverage/**"
			]
		}
	}
}
