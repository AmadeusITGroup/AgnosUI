{
	"name": "@agnos-ui/agnos-ui",
	"description": "Framework-agnostic widget library.",
	"version": "0.0.0",
	"homepage": "https://amadeusitgroup.github.io/AgnosUI/latest/",
	"bugs": "https://github.com/AmadeusITGroup/AgnosUI/issues",
	"license": "MIT",
	"private": true,
	"type": "module",
	"keywords": [
		"AgnosUI",
		"accordion",
		"alert",
		"components",
		"modal",
		"pagination",
		"progressbar",
		"rating",
		"slider",
		"toast",
		"widgets"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AmadeusITGroup/AgnosUI.git"
	},
	"engines": {
		"npm": "^10.5.1",
		"node": "^20.12.1"
	},
	"packageManager": "npm@10.5.1",
	"devDependencies": {
		"@axe-core/playwright": "^4.9.0",
		"@commitlint/cli": "^19.2.1",
		"@microsoft/api-extractor": "^7.43.0",
		"@playwright/test": "^1.43.0",
		"@types/node": "^20.12.5",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitest/browser": "^1.4.0",
		"@vitest/ui": "^1.4.0",
		"bootstrap": "^5.3.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jsdoc": "^48.2.3",
		"express": "^4.19.2",
		"glob": "10.3.12",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.2.5",
		"syncpack": "^12.3.0",
		"typescript": "~5.4.4",
		"vite": "^5.2.8",
		"vitest": "^1.4.0",
		"wireit": "^0.14.4"
	},
	"optionalDependencies": {
		"@esbuild/darwin-arm64": "0.20.2",
		"@rollup/rollup-darwin-arm64": "4.14.1",
		"@rollup/rollup-darwin-x64": "4.14.1",
		"@rollup/rollup-win32-x64-msvc": "4.14.1"
	},
	"scripts": {
		"build:coverage": "wireit",
		"build": "node scripts/run.js build serial",
		"build:ci": "wireit",
		"check": "wireit",
		"tdd": "node scripts/run.js tdd",
		"tdd:ui": "node scripts/run.js tdd:ui",
		"tdd:debug": "node scripts/run.js tdd:debug",
		"test:coverage": "node scripts/run.js test:coverage",
		"test": "node scripts/run.js test serial",
		"format:fix": "prettier . \"**/*.svelte\" --write --cache .",
		"format:check": "prettier . \"**/*.svelte\" --check --cache .",
		"lint": "wireit",
		"dev": "wireit",
		"dev:coverage": "wireit",
		"preview": "npm run preview -w demo",
		"preview:coverage": "npm run preview:coverage -w demo",
		"e2e": "wireit",
		"e2e:coverage": "wireit",
		"e2e:ui": "wireit",
		"postinstall": "husky",
		"syncpack:check": "syncpack lint",
		"syncpack:fix": "syncpack fix-mismatches && syncpack format",
		"prepare": "wireit"
	},
	"wireit": {
		"dev": {
			"dependencies": [
				"./angular/demo:dev",
				"./react/demo:dev",
				"./svelte/demo:dev",
				"./demo:dev"
			]
		},
		"dev:coverage": {
			"dependencies": [
				"./angular/demo:dev:coverage",
				"./react/demo:dev:coverage",
				"./svelte/demo:dev:coverage",
				"./demo:dev"
			]
		},
		"build:ci": {
			"dependencies": [
				"./code-coverage:build",
				"./demo:build",
				"./eslint-plugin:build",
				"./page-objects:build",
				"./demo/stackblitz:build",
				"./angular/ssr-app:build",
				"./react/ssr-app:build",
				"./svelte/ssr-app:build"
			]
		},
		"build:coverage": {
			"dependencies": [
				"./demo:build:coverage"
			]
		},
		"e2e": {
			"command": "node scripts/e2e.js"
		},
		"e2e:coverage": {
			"command": "node scripts/e2e.js --coverage",
			"dependencies": [
				"./code-coverage:build"
			]
		},
		"e2e:ui": {
			"command": "node scripts/e2e.js --ui",
			"service": true
		},
		"prepare": {
			"dependencies": [
				"./angular/headless:generate:exports",
				"./react/headless:generate:exports",
				"./react/bootstrap:generate:exports",
				"./svelte/headless:generate:exports",
				"./svelte/bootstrap:generate:exports"
			]
		},
		"check:ts-gen-doc": {
			"command": "tsc --project scripts/doc/tsconfig.json",
			"files": [
				"scripts/doc/**",
				"tsconfig.json"
			],
			"output": []
		},
		"check": {
			"dependencies": [
				"check:ts-gen-doc",
				"./core:check",
				"./eslint-plugin:check",
				"./react/demo:check",
				"./react/ssr-app:check",
				"./svelte/headless:check",
				"./svelte/bootstrap:check",
				"./svelte/demo:check",
				"./svelte/ssr-app:check",
				"./angular/bootstrap:check",
				"./demo:check"
			]
		},
		"lint": {
			"command": "eslint --ext mjs,js,ts,tsx,svelte .",
			"dependencies": [
				"./eslint-plugin:build",
				"./svelte/bootstrap:build"
			]
		}
	},
	"workspaces": [
		"code-coverage",
		"core",
		"core-bootstrap",
		"angular/headless",
		"angular/bootstrap",
		"angular/demo",
		"angular/ssr-app",
		"react/headless",
		"react/bootstrap",
		"react/demo",
		"react/ssr-app",
		"svelte/headless",
		"svelte/bootstrap",
		"svelte/demo",
		"svelte/ssr-app",
		"demo",
		"eslint-plugin",
		"base-po",
		"page-objects",
		"verdaccio"
	]
}
