{
	"name": "@agnos-ui/svelte-headless",
	"private": true,
	"description": "Headless widget library for Svelte.",
	"type": "module",
	"main": "./index.js",
	"module": "./index.js",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"svelte": "./index.js"
		},
		"./accordion": {
			"types": "./accordion.d.ts",
			"default": "./accordion.js"
		},
		"./alert": {
			"types": "./alert.d.ts",
			"default": "./alert.js"
		},
		"./config": {
			"types": "./config.d.ts",
			"default": "./config.js"
		},
		"./modal": {
			"types": "./modal.d.ts",
			"default": "./modal.js"
		},
		"./pagination": {
			"types": "./pagination.d.ts",
			"default": "./pagination.js"
		},
		"./progressbar": {
			"types": "./progressbar.d.ts",
			"default": "./progressbar.js"
		},
		"./rating": {
			"types": "./rating.d.ts",
			"default": "./rating.js"
		},
		"./select": {
			"types": "./select.d.ts",
			"default": "./select.js"
		},
		"./services": {
			"types": "./services.d.ts",
			"default": "./services.js"
		},
		"./slider": {
			"types": "./slider.d.ts",
			"default": "./slider.js"
		},
		"./Slot.svelte": {
			"types": "./Slot.svelte.d.ts",
			"default": "./Slot.svelte"
		},
		"./slotTypes": {
			"types": "./slotTypes.d.ts",
			"default": "./slotTypes.js"
		},
		"./types": {
			"types": "./types.d.ts",
			"default": "./types.js"
		},
		"./utils": {
			"types": "./utils.d.ts",
			"default": "./utils.js"
		}
	},
	"dependencies": {
		"@agnos-ui/core": "",
		"@amadeus-it-group/tansu": "0.0.23"
	},
	"peerDependencies": {
		"svelte": "*"
	},
	"sideEffects": false
}
