{
	"name": "@agnos-ui/svelte-bootstrap",
	"description": "Bootstrap-based widget library for Svelte.",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./components/*": {
			"types": "./dist/components/*/index.d.ts",
			"svelte": "./dist/components/*/index.js",
			"default": "./dist/components/*/index.js"
		},
		"./config": {
			"types": "./dist/config.d.ts",
			"svelte": "./dist/config.js",
			"default": "./dist/config.js"
		},
		"./slot": {
			"types": "./dist/slot.d.ts",
			"svelte": "./dist/slot.js",
			"default": "./dist/slot.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"svelte": "./dist/types.js",
			"default": "./dist/types.js"
		},
		"./services": {
			"types": "./dist/services/index.d.ts",
			"svelte": "./dist/services/index.js",
			"default": "./dist/services/index.js"
		},
		"./transitions": {
			"types": "./dist/transitions/index.d.ts",
			"svelte": "./dist/transitions/index.js",
			"default": "./dist/transitions/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"svelte": "./dist/utils/index.js",
			"default": "./dist/utils/index.js"
		}
	},
	"scripts": {
		"build": "wireit",
		"check": "wireit"
	},
	"wireit": {
		"generate:component-types": {
			"command": "tsx --tsconfig ../../scripts/bootstrapTypes/tsconfig.json ../../scripts/bootstrapTypes/genTypeExports.ts svelte",
			"files": [
				"../../core/src/**",
				"../../core-bootstrap/src/**",
				"../../scripts/bootstrapTypes/genTypeExports.ts"
			],
			"output": [
				"src/components/**/*.gen.ts"
			]
		},
		"build:src": {
			"command": "svelte-package -i src",
			"dependencies": [
				"generate:component-types",
				"../headless:build",
				"../../core-bootstrap:build",
				"../preprocess:build"
			],
			"files": [
				"svelte.config.js",
				"src/**",
				"!src/generated/**",
				"tsconfig.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/**",
				"!dist/README.md",
				"!dist/package.json"
			]
		},
		"build:pkg": {
			"command": "node ../../scripts/buildPackageJson.js . dist",
			"dependencies": [
				"build:src"
			],
			"files": [
				"../../scripts/buildPackageJson.js",
				"../../package.json",
				"package.json",
				"README.md"
			],
			"output": [
				"dist/README.md",
				"dist/package.json"
			]
		},
		"build": {
			"dependencies": [
				"build:pkg"
			]
		},
		"check": {
			"command": "svelte-check --fail-on-warnings --output human-verbose",
			"dependencies": [
				"generate:component-types",
				"../headless:build",
				"../../core-bootstrap:build"
			],
			"files": [
				"src/**",
				"tsconfig.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": []
		},
		"generate:typedoc": {
			"command": "typedoc",
			"dependencies": [
				"generate:component-types"
			],
			"files": [
				"src/**",
				"!src/generated/**",
				"tsconfig.typedoc.json",
				"tsconfig.json",
				"../tsconfig.json",
				"../../tsconfig.json",
				"typedoc.json",
				"../../typedoc.base.json"
			],
			"output": [
				"generated/md/**",
				"generated/svelte-bootstrap.json"
			]
		}
	},
	"dependencies": {
		"@agnos-ui/core-bootstrap": "0.0.0",
		"@agnos-ui/svelte-headless": "0.0.0"
	},
	"peerDependencies": {
		"@amadeus-it-group/tansu": "^1.0.0",
		"esm-env": "^1.0.0",
		"svelte": "^4.2.18"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.2"
	},
	"sideEffects": false
}
