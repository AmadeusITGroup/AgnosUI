{
	"name": "@agnos-ui/svelte-ssr-app",
	"version": "0.6.0-next.0",
	"private": true,
	"scripts": {
		"dev": "wireit",
		"build": "wireit",
		"preview": "wireit",
		"check": "wireit"
	},
	"dependencies": {
		"@agnos-ui/svelte-bootstrap": "0.6.0-next.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.8.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"svelte": "^5.2.7",
		"svelte-check": "^4.1.0"
	},
	"type": "module",
	"wireit": {
		"sveltekit": {
			"command": "svelte-kit sync",
			"dependencies": [
				"../preprocess:build"
			],
			"files": [
				"src/**",
				"svelte.config.js"
			],
			"output": [
				".svelte-kit/**",
				"!.svelte-kit/{generated,output}/**"
			]
		},
		"deps": {
			"dependencies": [
				"../bootstrap:build",
				"../preprocess:build"
			]
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"sveltekit",
				"deps"
			],
			"files": [
				"tsconfig.json",
				"vite.config.ts"
			],
			"output": [
				".svelte-kit/{generated,output}/**"
			]
		},
		"check": {
			"command": "svelte-check --tsconfig ./tsconfig.json",
			"dependencies": [
				"sveltekit",
				"deps"
			],
			"files": [
				"tsconfig.json"
			],
			"output": []
		},
		"dev": {
			"command": "vite dev",
			"dependencies": [
				"../bootstrap:generate:exports",
				"../headless:generate:exports",
				"../preprocess:build"
			],
			"service": true
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build"
			],
			"service": true
		}
	}
}
