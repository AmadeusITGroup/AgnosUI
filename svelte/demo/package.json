{
	"name": "@agnos-ui/svelte-demo",
	"version": "0.7.1",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "wireit",
		"dev:coverage": "wireit",
		"build": "wireit",
		"build:coverage": "wireit",
		"check": "wireit"
	},
	"wireit": {
		"dev": {
			"command": "vite",
			"service": true,
			"dependencies": [
				"../preprocess:build",
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			]
		},
		"dev:coverage": {
			"command": "vite -c vite.coverage.config.ts",
			"service": true,
			"dependencies": [
				"../../code-coverage:build",
				"../preprocess:build",
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			]
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"../preprocess:build",
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"build:coverage": {
			"command": "vite build -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"../preprocess:build",
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			],
			"files": [
				"src/**",
				"vite.config.ts",
				"vite.coverage.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "svelte-check --fail-on-warnings --output human-verbose --tsconfig tsconfig.check.json",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"tsconfig.check.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": []
		}
	},
	"dependencies": {
		"@agnos-ui/core-bootstrap": "0.7.1",
		"@agnos-ui/svelte-bootstrap": "0.7.1",
		"@amadeus-it-group/tansu": "^2.0.0",
		"@floating-ui/dom": "^1.6.12"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.1",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.20",
		"postcss": "^8.4.49",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.16"
	}
}
