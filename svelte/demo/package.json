{
	"name": "@agnos-ui/svelte-demo",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "wireit",
		"dev:coverage": "wireit",
		"build": "wireit",
		"build:coverage": "wireit",
		"check": "wireit"
	},
	"wireit": {
		"dev": {
			"command": "vite",
			"service": true,
			"dependencies": [
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			]
		},
		"dev:coverage": {
			"command": "vite -c vite.coverage.config.ts",
			"service": true,
			"dependencies": [
				"../../code-coverage:build",
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			]
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"build:coverage": {
			"command": "vite build -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			],
			"files": [
				"src/**",
				"vite.config.ts",
				"vite.coverage.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "svelte-check --fail-on-warnings --output human-verbose",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"tsconfig.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": []
		}
	},
	"dependencies": {
		"@agnos-ui/core-bootstrap": "0.0.0",
		"@agnos-ui/svelte-bootstrap": "0.0.0",
		"@amadeus-it-group/tansu": "1.0.0",
		"@floating-ui/dom": "^1.6.3"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.10.1",
		"eslint-plugin-svelte": "^2.36.0",
		"postcss": "^8.4.38",
		"prettier-plugin-svelte": "^3.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3"
	}
}
