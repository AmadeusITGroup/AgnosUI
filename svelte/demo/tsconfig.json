{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"paths": {
			"@agnos-ui/core/*": ["./core/src/*"],
			"@agnos-ui/svelte-headless/*": ["./svelte/headless/src/*", "./svelte/headless/src/generated/*"],
			"@agnos-ui/svelte-bootstrap/*": ["./svelte/bootstrap/src/*", "./svelte/bootstrap/src/generated/*"],
			"@agnos-ui/common": ["./common"],
			"@agnos-ui/common/*": ["./common/*"],
			"@agnos-ui/code-coverage/*": ["./code-coverage/lib/*"]
		},
		"allowJs": true
	},
	"include": ["src"],
	"files": ["vite.config.ts", "vite.coverage.config.ts"]
}
