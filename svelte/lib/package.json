{
	"name": "@agnos-ui/svelte",
	"private": true,
	"description": "Bootstrap-based widget library for Svelte.",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"scripts": {
		"build:package": "svelte-package -i src && node ../../scripts/buildPackageJson.js . dist",
		"build": "npm run build:package && npm run check",
		"check": "svelte-check --fail-on-warnings --output human-verbose"
	},
	"dependencies": {
		"@agnos-ui/svelte-headless": "",
		"@agnos-ui/style-bootstrap": "",
		"@amadeus-it-group/tansu": "0.0.24"
	},
	"peerDependencies": {
		"svelte": "*"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.2.3"
	},
	"sideEffects": false
}
