{
	"name": "@agnos-ui/svelte",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite -c vite.demo.config.ts",
		"dev:coverage": "vite -c vite.demo-coverage.config.ts",
		"build": "npm run build:headless && npm run build:lib && npm run build:demo && npm run svelte-check:headless && npm run svelte-check:lib && npm run svelte-check:demo && npm run svelte-check:demo-build",
		"build:headless": "cd headless && svelte-package -i . -o ../dist/headless && node ../../scripts/rm.js ../dist/headless/tsconfig.json && node ../../scripts/buildPackageJson.js . ../dist/headless",
		"build:lib": "cd lib && svelte-package -i . -o ../dist/lib && node ../../scripts/rm.js ../dist/lib/tsconfig.json && node ../../scripts/buildPackageJson.js . ../dist/lib",
		"build:coverage": "vite build -c vite.demo-coverage.config.ts",
		"build:demo": "vite build -c vite.demo.config.ts",
		"preview": "vite preview -c vite.demo.config.ts",
		"svelte-check:headless": "svelte-check --fail-on-warnings --tsconfig ./headless/tsconfig.json --output human-verbose",
		"svelte-check:lib": "svelte-check --fail-on-warnings --tsconfig ./lib/tsconfig.json --output human-verbose",
		"svelte-check:demo": "svelte-check --fail-on-warnings --tsconfig ./demo/tsconfig.json --output human-verbose",
		"svelte-check:demo-build": "svelte-check --fail-on-warnings --tsconfig ./demo/tsconfig.build.json --output human-verbose"
	},
	"dependencies": {
		"@amadeus-it-group/tansu": "0.0.23"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.2.2",
		"@sveltejs/vite-plugin-svelte": "^2.4.6",
		"eslint-plugin-svelte": "^2.34.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2"
	}
}
