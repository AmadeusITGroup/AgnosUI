{
	"compilerOptions": {
		"baseUrl": "./",
		"target": "es2022",
		"module": "esnext",
		"strict": true,
		"skipLibCheck": true,
		"noImplicitReturns": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"paths": {
			"@agnos-ui/common": ["./common"],
			"@agnos-ui/common/*": ["./common/*"],
		},
	},
}
