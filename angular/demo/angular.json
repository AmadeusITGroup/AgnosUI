{
	"$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "",
	"projects": {
		"bootstrap": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": ".",
			"sourceRoot": "bootstrap/src",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": {
							"base": "dist/bootstrap",
							"browser": ""
						},
						"index": "bootstrap/src/index.html",
						"browser": "bootstrap/src/main.ts",
						"polyfills": ["bootstrap/src/polyfills.ts"],
						"tsConfig": "tsconfig.build.json",
						"assets": ["bootstrap/src/agnosui-logo.svg"],
						"styles": ["bootstrap/src/styles.css", "../../common/demo.scss", "@agnos-ui/style-bootstrap/scss/agnosui.scss"],
						"inlineStyleLanguage": "scss",
						"loader": {
							".txt": "text",
							".svg": "text"
						}
					},
					"configurations": {
						"coverage": {
							"fileReplacements": [
								{
									"replace": "bootstrap/src/environments/environment.ts",
									"with": "bootstrap/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"tsConfig": "bootstrap/tsconfig.json"
						},
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "bootstrap/src/environments/environment.ts",
									"with": "bootstrap/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"tsConfig": "bootstrap/tsconfig.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"servePath": "/angular/samples/bootstrap",
							"buildTarget": "bootstrap:build:production"
						},
						"development": {
							"servePath": "/angular/samples/bootstrap",
							"buildTarget": "bootstrap:build:development",
							"prebundle": false
						}
					},
					"defaultConfiguration": "development"
				}
			}
		},
		"daisyui": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": ".",
			"sourceRoot": "daisyui/src",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": {
							"base": "dist/daisyui",
							"browser": ""
						},
						"index": "daisyui/src/index.html",
						"browser": "daisyui/src/main.ts",
						"polyfills": ["daisyui/src/polyfills.ts"],
						"tsConfig": "tsconfig.build.json",
						"assets": ["daisyui/src/agnosui-logo.svg"],
						"styles": ["daisyui/src/styles.css", "../../common/demo.scss"],
						"inlineStyleLanguage": "scss"
					},
					"configurations": {
						"coverage": {
							"fileReplacements": [
								{
									"replace": "daisyui/src/environments/environment.ts",
									"with": "daisyui/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"tsConfig": "daisyui/tsconfig.json"
						},
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "daisyui/src/environments/environment.ts",
									"with": "daisyui/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"tsConfig": "daisyui/tsconfig.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"servePath": "/angular/samples/daisyui",
							"buildTarget": "daisyui:build:production"
						},
						"development": {
							"servePath": "/angular/samples/daisyui",
							"buildTarget": "daisyui:build:development",
							"prebundle": false
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201
					}
				}
			}
		}
	},
	"cli": {
		"analytics": false
	}
}
