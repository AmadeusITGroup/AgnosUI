{
	"name": "@agnos-ui/angular-demo",
	"private": true,
	"type": "module",
	"scripts": {
		"ng": "ng",
		"dev": "wireit",
		"dev:coverage": "wireit",
		"build": "wireit",
		"build:coverage": "wireit"
	},
	"wireit": {
		"generate:links": {
			"command": "node scripts/regen-links.mjs",
			"files": [
				"scripts/regen-links.mjs",
				"bootstrap/src/app/samples/**",
				"daisyui/src/app/samples/**"
			],
			"output": [
				"bootstrap/src/app/generated/samples.route.ts",
				"daisyui/src/app/generated/samples.route.ts"
			]
		},
		"dev:bootstrap": {
			"command": "ng serve bootstrap",
			"dependencies": [
				"../headless:generate:exports",
				"generate:links"
			],
			"service": true
		},
		"dev:daisyui": {
			"command": "ng serve daisyui",
			"dependencies": [
				"../headless:generate:exports",
				"generate:links"
			],
			"service": true
		},
		"dev": {
			"dependencies": [
				"dev:bootstrap",
				"dev:daisyui"
			]
		},
		"dev:bootstrap:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js serve bootstrap",
			"dependencies": [
				"../../code-coverage:build",
				"../headless:generate:exports",
				"generate:links"
			],
			"service": true
		},
		"dev:daisyui:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js serve daisyui",
			"dependencies": [
				"../../code-coverage:build",
				"../headless:generate:exports",
				"generate:links"
			],
			"service": true
		},
		"dev:coverage": {
			"dependencies": [
				"dev:bootstrap:coverage",
				"dev:daisyui:coverage"
			]
		},
		"build:bootstrap": {
			"command": "ng build bootstrap",
			"dependencies": [
				"generate:links",
				"../lib:build",
				"../../style-bootstrap:build"
			],
			"files": [
				"bootstrap/src/**",
				"angular.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/bootstrap/**"
			]
		},
		"build:daisyui": {
			"command": "ng build daisyui",
			"dependencies": [
				"generate:links",
				"../lib:build"
			],
			"files": [
				"daisyui/src/**",
				"angular.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/daisyui/**"
			]
		},
		"build": {
			"dependencies": [
				"build:bootstrap",
				"build:daisyui"
			]
		},
		"build:bootstrap:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js build bootstrap --configuration=coverage",
			"dependencies": [
				"generate:links",
				"../lib:build",
				"../../style-bootstrap:build",
				"../../code-coverage:build"
			],
			"files": [
				"bootstrap/src/**",
				"angular.json",
				"bootstrap/tsconfig.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/bootstrap/**"
			]
		},
		"build:daisyui:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js build daisyui --configuration=coverage",
			"dependencies": [
				"generate:links",
				"../lib:build",
				"../../code-coverage:build"
			],
			"files": [
				"daisyui/src/**",
				"daisyui/tsconfig.json",
				"angular.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/daisyui/**"
			]
		},
		"build:coverage": {
			"dependencies": [
				"build:bootstrap:coverage",
				"build:daisyui:coverage"
			]
		}
	},
	"dependencies": {
		"@agnos-ui/angular": "",
		"@agnos-ui/angular-headless": "",
		"@agnos-ui/core": "",
		"@amadeus-it-group/tansu": "1.0.0",
		"@agnos-ui/style-bootstrap": "",
		"@angular/animations": "~17.3.0",
		"@angular/common": "~17.3.0",
		"@angular/compiler": "~17.3.0",
		"@angular/core": "~17.3.0",
		"@angular/forms": "~17.3.0",
		"@angular/platform-browser": "~17.3.0",
		"@angular/platform-browser-dynamic": "~17.3.0",
		"@angular/router": "~17.3.0",
		"@floating-ui/dom": "^1.6.3",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.2",
		"zone.js": "~0.14.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~17.3.0",
		"@angular-eslint/eslint-plugin": "^17.3.0",
		"@angular-eslint/eslint-plugin-template": "^17.3.0",
		"@angular-eslint/template-parser": "^17.3.0",
		"@angular/cli": "~17.3.0",
		"@angular/compiler-cli": "~17.3.0",
		"autoprefixer": "^10.4.18",
		"sirv-cli": "^2.0.2",
		"daisyui": "^4.7.3",
		"postcss": "^8.4.36",
		"tailwindcss": "^3.4.1",
		"typescript": "~5.4.2"
	}
}
