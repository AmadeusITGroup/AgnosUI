{
	"name": "@agnos-ui/angular-demo",
	"version": "0.10.0",
	"private": true,
	"type": "module",
	"scripts": {
		"ng": "ng",
		"dev": "wireit",
		"dev:bootstrap": "wireit",
		"dev:daisyui": "wireit",
		"dev:coverage": "wireit",
		"dev:bootstrap:coverage": "wireit",
		"dev:daisyui:coverage": "wireit",
		"build": "wireit",
		"build:coverage": "wireit"
	},
	"wireit": {
		"generate:links": {
			"command": "node scripts/regen-links.js",
			"files": [
				"scripts/regen-links.js",
				"bootstrap/src/app/samples/**",
				"daisyui/src/app/samples/**"
			],
			"output": [
				"bootstrap/src/app/generated/samples.route.ts",
				"daisyui/src/app/generated/samples.route.ts"
			]
		},
		"dev:bootstrap": {
			"command": "ng serve bootstrap",
			"dependencies": [
				"../headless:generate:exports",
				"../bootstrap:generate:component-types",
				"generate:links"
			],
			"service": true
		},
		"dev:daisyui": {
			"command": "ng serve daisyui",
			"dependencies": [
				"../headless:generate:exports",
				"generate:links"
			],
			"service": true
		},
		"dev": {
			"dependencies": [
				"dev:bootstrap",
				"dev:daisyui"
			]
		},
		"dev:bootstrap:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js serve bootstrap",
			"dependencies": [
				"../../code-coverage:build",
				"../headless:generate:exports",
				"../bootstrap:generate:component-types",
				"generate:links"
			],
			"service": true
		},
		"dev:daisyui:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js serve daisyui",
			"dependencies": [
				"../../code-coverage:build",
				"../headless:generate:exports",
				"generate:links"
			],
			"service": true
		},
		"dev:coverage": {
			"dependencies": [
				"dev:bootstrap:coverage",
				"dev:daisyui:coverage"
			]
		},
		"build:bootstrap": {
			"command": "ng build bootstrap",
			"dependencies": [
				"generate:links",
				"../bootstrap:build"
			],
			"files": [
				"bootstrap/src/**",
				"angular.json",
				"bootstrap/tsconfig.build.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/bootstrap/**"
			]
		},
		"build:daisyui": {
			"command": "ng build daisyui",
			"dependencies": [
				"generate:links",
				"../headless:build"
			],
			"files": [
				"daisyui/src/**",
				".postcssrc.json",
				"daisyui/tsconfig.build.json",
				"angular.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/daisyui/**"
			]
		},
		"build": {
			"dependencies": [
				"build:bootstrap",
				"build:daisyui"
			]
		},
		"build:bootstrap:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js build bootstrap --configuration=coverage",
			"dependencies": [
				"generate:links",
				"../bootstrap:build",
				"../../code-coverage:build"
			],
			"files": [
				"bootstrap/src/**",
				"angular.json",
				"bootstrap/tsconfig.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/bootstrap/**"
			]
		},
		"build:daisyui:coverage": {
			"command": "node --import @agnos-ui/code-coverage/interceptReadFile ../../node_modules/@angular/cli/bin/ng.js build daisyui --configuration=coverage",
			"dependencies": [
				"generate:links",
				"../bootstrap:build",
				"../../code-coverage:build"
			],
			"files": [
				"daisyui/src/**",
				".postcssrc.json",
				"daisyui/tsconfig.json",
				"angular.json",
				"tsconfig.build.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/daisyui/**"
			]
		},
		"build:coverage": {
			"dependencies": [
				"build:bootstrap:coverage",
				"build:daisyui:coverage"
			]
		}
	},
	"dependencies": {
		"@agnos-ui/angular-bootstrap": "0.10.0",
		"@agnos-ui/core-bootstrap": "0.10.0",
		"@amadeus-it-group/tansu": "^2.0.0",
		"@angular/animations": "^21.0.0",
		"@angular/common": "^21.0.0",
		"@angular/compiler": "^21.0.0",
		"@angular/core": "^21.0.0",
		"@angular/forms": "^21.0.0",
		"@angular/platform-browser": "^21.0.0",
		"@angular/router": "^21.0.0",
		"@floating-ui/dom": "^1.6.12",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@angular/build": "^21.0.0",
		"@angular/cli": "^21.0.0",
		"@angular/compiler-cli": "^21.0.0",
		"@tailwindcss/postcss": "^4.0.16",
		"bootstrap": "^5.3.3",
		"bootstrap-icons": "^1.11.3",
		"daisyui": "^5.0.6",
		"embla-carousel-autoplay": "^8.5.2",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.0.0",
		"typescript": "~5.9.0"
	}
}
