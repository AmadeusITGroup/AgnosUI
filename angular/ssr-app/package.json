{
	"name": "@agnos-ui/angular-ssr-app",
	"version": "0.10.0",
	"scripts": {
		"ng": "ng",
		"dev": "ng serve",
		"build": "wireit",
		"preview": "wireit"
	},
	"private": true,
	"dependencies": {
		"@agnos-ui/angular-bootstrap": "0.10.0",
		"@angular/animations": "^21.0.0",
		"@angular/common": "^21.0.0",
		"@angular/compiler": "^21.0.0",
		"@angular/core": "^21.0.0",
		"@angular/forms": "^21.0.0",
		"@angular/platform-browser": "^21.0.0",
		"@angular/platform-server": "^21.0.0",
		"@angular/router": "^21.0.0",
		"@angular/ssr": "^21.0.0",
		"express": "^5.1.0",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@angular/build": "^21.0.0",
		"@angular/cli": "^21.0.0",
		"@angular/compiler-cli": "^21.0.0",
		"@types/express": "^5.0.0",
		"@types/node": "^24.0.0",
		"typescript": "~5.9.0"
	},
	"wireit": {
		"build": {
			"command": "ng build",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"angular.json",
				"server.ts",
				"tsconfig.app.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/**"
			]
		},
		"preview": {
			"command": "node dist/ssr-app/server/server.mjs",
			"dependencies": [
				"build"
			],
			"service": true
		},
		"dev": {
			"command": "ng serve",
			"dependencies": [
				"../headless:generate:exports"
			],
			"service": true
		}
	}
}
