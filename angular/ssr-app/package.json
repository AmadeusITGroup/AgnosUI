{
	"name": "@agnos-ui/angular-ssr-app",
	"version": "0.6.0",
	"scripts": {
		"ng": "ng",
		"dev": "ng serve",
		"build": "wireit",
		"preview": "wireit"
	},
	"private": true,
	"dependencies": {
		"@agnos-ui/angular-bootstrap": "0.6.0",
		"@angular/animations": "^18.2.12",
		"@angular/common": "^18.2.12",
		"@angular/compiler": "^18.2.12",
		"@angular/core": "^18.2.12",
		"@angular/forms": "^18.2.12",
		"@angular/platform-browser": "^18.2.12",
		"@angular/platform-browser-dynamic": "^18.2.12",
		"@angular/platform-server": "^18.2.12",
		"@angular/router": "^18.2.12",
		"@angular/ssr": "^18.2.12",
		"express": "^4.21.1",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.2.12",
		"@angular/cli": "^18.2.12",
		"@angular/compiler-cli": "^18.2.12",
		"@types/express": "^5.0.0",
		"@types/node": "^22.10.1",
		"typescript": "~5.5.4"
	},
	"wireit": {
		"build": {
			"command": "ng build",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"angular.json",
				"server.ts",
				"tsconfig.app.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/**"
			]
		},
		"preview": {
			"command": "node dist/ssr-app/server/server.mjs",
			"dependencies": [
				"build"
			],
			"service": true
		},
		"dev": {
			"command": "ng serve",
			"dependencies": [
				"../headless:generate:exports"
			],
			"service": true
		}
	}
}
