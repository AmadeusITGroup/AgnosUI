{
	"name": "@agnos-ui/angular-ssr-app",
	"version": "0.4.0-next.0",
	"scripts": {
		"ng": "ng",
		"dev": "ng serve",
		"build": "wireit",
		"preview": "wireit"
	},
	"private": true,
	"dependencies": {
		"@agnos-ui/angular-bootstrap": "0.4.0-next.0",
		"@angular/animations": "^18.0.4",
		"@angular/common": "^18.0.4",
		"@angular/compiler": "^18.0.4",
		"@angular/core": "^18.0.4",
		"@angular/forms": "^18.0.4",
		"@angular/platform-browser": "^18.0.4",
		"@angular/platform-browser-dynamic": "^18.0.4",
		"@angular/platform-server": "^18.0.4",
		"@angular/router": "^18.0.4",
		"@angular/ssr": "^18.0.5",
		"express": "^4.19.2",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.3",
		"zone.js": "~0.14.7"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.0.5",
		"@angular/cli": "^18.0.5",
		"@angular/compiler-cli": "^18.0.4",
		"@types/express": "^4.17.21",
		"@types/jasmine": "~5.1.4",
		"@types/node": "^20.14.8",
		"jasmine-core": "~5.1.2",
		"karma": "~6.4.3",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"typescript": "~5.4.5"
	},
	"wireit": {
		"build": {
			"command": "ng build",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"angular.json",
				"server.ts",
				"tsconfig.app.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [
				"dist/**"
			]
		},
		"preview": {
			"command": "node dist/ssr-app/server/server.mjs",
			"dependencies": [
				"build"
			],
			"service": true
		},
		"dev": {
			"command": "ng serve",
			"dependencies": [
				"../headless:generate:exports"
			],
			"service": true
		}
	}
}
