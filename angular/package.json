{
	"name": "@agnos-ui/angular",
	"private": true,
	"scripts": {
		"ng": "ng",
		"dev": "ng serve",
		"dev:coverage": "ng serve --configuration coverage",
		"build": "npm run build:lib && npm run build:demo",
		"build:lib": "ng build lib",
		"build:demo": "ng build demo",
		"build:coverage": "ng build demo --configuration coverage",
		"watch": "ng build --watch --configuration development",
		"preview": "node ./scripts/preview.cjs dist/demo --port 4200 --single",
		"tdd": "npm run test:lib --watch",
		"test": "npm run test:lib",
		"test:lib": "ng test lib",
		"test:demo": "ng test demo"
	},
	"dependencies": {
		"@angular/animations": "^16.1.8",
		"@angular/common": "^16.1.8",
		"@angular/compiler": "^16.1.8",
		"@angular/core": "^16.1.8",
		"@angular/forms": "^16.1.8",
		"@angular/platform-browser": "^16.1.8",
		"@angular/platform-browser-dynamic": "^16.1.8",
		"@angular/router": "^16.1.8",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.1",
		"zone.js": "^0.13.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^16.1.7",
		"@angular-eslint/eslint-plugin": "^16.1.0",
		"@angular-eslint/eslint-plugin-template": "^16.1.0",
		"@angular-eslint/template-parser": "^16.1.0",
		"@angular/cli": "^16.1.7",
		"@angular/compiler-cli": "^16.1.8",
		"@types/jasmine": "^4.3.5",
		"@types/webpack-env": "^1.18.1",
		"coverage-istanbul-loader": "^3.0.5",
		"jasmine-core": "^5.1.0",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-jasmine": "^5.1.0",
		"karma-jasmine-html-reporter": "^2.1.0",
		"ng-packagr": "^16.1.0",
		"ngx-build-plus": "^16.0.0",
		"raw-loader": "^4.0.2",
		"sirv-cli": "^2.0.2",
		"typescript": "~5.1.6"
	}
}
