{
	"name": "@agnos-ui/react-demo",
	"version": "0.8.0-next.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "wireit",
		"dev:coverage": "wireit",
		"build:coverage": "wireit",
		"build": "wireit",
		"check": "wireit"
	},
	"wireit": {
		"dev": {
			"command": "vite",
			"dependencies": [
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			],
			"service": true
		},
		"dev:coverage": {
			"command": "vite -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			],
			"service": true
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"build:coverage": {
			"command": "vite build -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			],
			"files": [
				"src/**",
				"vite.config.ts",
				"vite.coverage.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "tsc",
			"files": [
				"src/**",
				"tsconfig.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": [],
			"dependencies": [
				"../bootstrap:build"
			]
		}
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.0.6",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^4.3.4",
		"daisyui": "^5.0.0-beta.8",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.0.2",
		"tailwindcss": "^4.0.0",
		"vite-plugin-svgr": "^4.3.0"
	},
	"dependencies": {
		"@agnos-ui/core-bootstrap": "0.8.0-next.1",
		"@agnos-ui/react-bootstrap": "0.8.0-next.1",
		"@amadeus-it-group/tansu": "^2.0.0",
		"@floating-ui/dom": "^1.6.12",
		"clsx": "^2.1.1"
	},
	"sideEffects": false
}
