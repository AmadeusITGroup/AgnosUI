{
	"name": "@agnos-ui/react-demo",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "wireit",
		"dev:coverage": "wireit",
		"build:coverage": "wireit",
		"build": "wireit",
		"check": "wireit"
	},
	"wireit": {
		"dev": {
			"command": "vite",
			"dependencies": [
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			],
			"service": true
		},
		"dev:coverage": {
			"command": "vite -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			],
			"service": true
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"build:coverage": {
			"command": "vite build -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			],
			"files": [
				"src/**",
				"vite.config.ts",
				"vite.coverage.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "tsc -p tsconfig.check.json",
			"files": [
				"src/**",
				"tsconfig.check.json"
			],
			"output": [],
			"dependencies": [
				"../bootstrap:build"
			]
		}
	},
	"devDependencies": {
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.19",
		"classnames": "^2.5.1",
		"daisyui": "^4.12.7",
		"eslint-plugin-react": "^7.34.3",
		"postcss": "^8.4.38",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.23.1",
		"tailwindcss": "^3.4.4",
		"vite-plugin-svgr": "^4.2.0"
	},
	"dependencies": {
		"@agnos-ui/core-bootstrap": "0.0.0",
		"@agnos-ui/react-bootstrap": "0.0.0",
		"@amadeus-it-group/tansu": "^1.0.0",
		"@floating-ui/dom": "^1.6.5",
		"classnames": "^2.5.1"
	},
	"peerDependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"sideEffects": false
}
