{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"paths": {
			"@agnos-ui/core/*": ["./core/src/*"],
			"@agnos-ui/react-headless/*": ["./react/headless/src/*", "./react/headless/src/generated/*"],
			"@agnos-ui/react-bootstrap/*": ["./react/bootstrap/src/*", "./react/bootstrap/src/generated/*"],
			"@agnos-ui/common": ["./common"],
			"@agnos-ui/common/*": ["./common/*"],
			"@agnos-ui/code-coverage/*": ["./code-coverage/lib/*"]
		},
		"allowJs": true
	},
	"include": ["src"],
	"files": ["vite.config.ts", "vite.coverage.config.ts"]
}
