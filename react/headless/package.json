{
	"name": "@agnos-ui/react-headless",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./components/*": {
			"types": "./dist/components/*/*.d.ts",
			"default": "./dist/components/*/*.js"
		},
		"./config": {
			"types": "./dist/config.d.ts",
			"default": "./dist/config.js"
		},
		"./slot": {
			"types": "./dist/slot.d.ts",
			"default": "./dist/slot.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		},
		"./services/*": {
			"types": "./dist/generated/services/*.d.ts",
			"default": "./dist/generated/services/*.js"
		},
		"./services/transitions/*": {
			"types": "./dist/generated/services/transitions/*.d.ts",
			"default": "./dist/generated/services/transitions/*.js"
		},
		"./utils/*": {
			"types": "./dist/generated/utils/*.d.ts",
			"default": "./dist/generated/utils/*.js"
		}
	},
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"generate:exports": {
			"command": "node ../../scripts/generateExports.js react/headless core @agnos-ui/core",
			"files": [
				"src/**",
				"../../core/src/**",
				"!src/generated/**",
				"../../scripts/generateExports.js"
			],
			"output": [
				"src/generated/**"
			]
		},
		"build:src": {
			"command": "tsc -p tsconfig.build.json",
			"dependencies": [
				"generate:exports",
				"../../core:build"
			],
			"files": [
				"src/**",
				"!src/generated/**",
				"tsconfig.build.json"
			],
			"output": [
				"dist/**",
				"!dist/README.md",
				"!dist/package.json"
			]
		},
		"build:pkg": {
			"command": "node ../../scripts/buildPackageJson.js . dist",
			"files": [
				"../../scripts/buildPackageJson.js",
				"package.json",
				"README.md"
			],
			"output": [
				"dist/README.md",
				"dist/package.json"
			]
		},
		"build": {
			"dependencies": [
				"build:src",
				"build:pkg"
			]
		}
	},
	"dependencies": {
		"@agnos-ui/core": "",
		"classnames": "^2.5.1"
	},
	"peerDependencies": {
		"@amadeus-it-group/tansu": "*",
		"react": "*",
		"react-dom": "*"
	},
	"devDependencies": {
		"@types/react": "^18.2.58",
		"@types/react-dom": "^18.2.19"
	},
	"sideEffects": false
}
