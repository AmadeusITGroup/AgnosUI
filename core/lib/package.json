{
	"private": true,
	"name": "@agnos-ui/core",
	"description": "Framework-agnostic headless widget library.",
	"keywords": [
		"agnostic"
	],
	"type": "module",
	"main": "./index.js",
	"module": "./index.js",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./index.js"
		},
		"./components/*": {
			"types": "./components/*/*.d.ts",
			"default": "./components/*/*.js"
		},
		"./config": {
			"types": "./config.d.ts",
			"default": "./config.js"
		},
		"./types": {
			"types": "./types.d.ts",
			"default": "./types.js"
		},
		"./services/*": {
			"types": "./services/*.d.ts",
			"default": "./services/*.js"
		},
		"./services/transitions/*": {
			"types": "./services/transitions/*.d.ts",
			"default": "./services/transitions/*.js"
		},
		"./utils/*": {
			"types": "./utils/*.d.ts",
			"default": "./utils/*.js"
		}
	},
	"dependencies": {
		"@amadeus-it-group/tansu": "0.0.24",
		"@floating-ui/dom": "^1.5.3"
	},
	"sideEffects": false
}
