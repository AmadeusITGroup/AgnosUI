{
	"private": true,
	"name": "@agnos-ui/core",
	"description": "Framework-agnostic headless widget library.",
	"keywords": [
		"agnostic"
	],
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./components/*": {
			"types": "./dist/components/*/*.d.ts",
			"default": "./dist/components/*/*.js"
		},
		"./config": {
			"types": "./dist/config.d.ts",
			"default": "./dist/config.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		},
		"./services/*": {
			"types": "./dist/services/*.d.ts",
			"default": "./dist/services/*.js"
		},
		"./services/transitions/*": {
			"types": "./dist/services/transitions/*.d.ts",
			"default": "./dist/services/transitions/*.js"
		},
		"./utils/*": {
			"types": "./dist/utils/*.d.ts",
			"default": "./dist/utils/*.js"
		}
	},
	"scripts": {
		"build": "npm run build:lib && npm run build:tsc && npm run build:api-extractor",
		"build:lib": "node ../scripts/rm.js dist && tsc && node ../scripts/buildPackageJson.js . dist",
		"build:tsc": "tsc -p tsconfig.check.json",
		"build:api-extractor": "api-extractor run",
		"test": "vitest run",
		"tdd": "vitest",
		"tdd:ui": "vitest --ui",
		"test:coverage": "node -r @agnos-ui/code-coverage/interceptReadFile ../node_modules/vitest/vitest.mjs run --coverage"
	},
	"dependencies": {
		"@amadeus-it-group/tansu": "0.0.24"
	},
	"devDependencies": {
		"eslint-plugin-jsdoc": "^46.9.1"
	},
	"peerDependencies": {
		"@floating-ui/dom": "*"
	},
	"sideEffects": false
}
