{
	"name": "@agnos-ui/demo",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "wireit",
		"dev:coverage": "wireit dev",
		"build": "wireit",
		"generate:doc": "wireit",
		"build:coverage": "wireit",
		"preview": "wireit",
		"preview:coverage": "wireit",
		"check": "wireit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"tdd": "vitest",
		"tdd:ui": "vitest --ui"
	},
	"wireit": {
		"generate:doc": {
			"command": "tsx --tsconfig ../scripts/doc/tsconfig.json ../scripts/doc/generate-docs.ts",
			"files": [
				"../core/src/**",
				"../core-bootstrap/src/**",
				"../scripts/doc/tsconfig.json",
				"../scripts/doc/generate-docs.ts",
				"../scripts/doc/api-doc.ts"
			],
			"output": [
				"generated/**"
			]
		},
		"dev": {
			"command": "vite dev",
			"dependencies": [
				"generate:doc"
			],
			"service": true
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"generate:doc",
				"../angular/demo:build",
				"../react/demo:build",
				"../svelte/demo:build"
			],
			"files": [
				"src/**",
				"static/**",
				"svelte.config.js",
				"vite.config.ts",
				"vite-env.d.ts",
				"scripts/*"
			],
			"output": [
				"dist/**",
				".svelte-kit/**"
			]
		},
		"build:coverage": {
			"command": "vite build",
			"dependencies": [
				"generate:doc",
				"../angular/demo:build:coverage",
				"../react/demo:build:coverage",
				"../svelte/demo:build:coverage",
				"../svelte/bootstrap:build"
			],
			"files": [
				"src/**",
				"static/**",
				"svelte.config.js",
				"vite.config.ts",
				"vite-env.d.ts",
				"scripts/*"
			],
			"output": [
				"dist/**",
				".svelte-kit/**"
			]
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build"
			],
			"service": true
		},
		"preview:coverage": {
			"command": "vite preview",
			"dependencies": [
				"build:coverage"
			],
			"service": true
		},
		"check": {
			"command": "svelte-check",
			"dependencies": [
				"build"
			],
			"files": [
				"src/**",
				"tsconfig.json"
			],
			"output": []
		}
	},
	"dependencies": {
		"@agnos-ui/svelte-bootstrap": "0.0.0",
		"@amadeus-it-group/tansu": "1.0.0",
		"@floating-ui/dom": "^1.6.3"
	},
	"devDependencies": {
		"@stackblitz/sdk": "^1.9.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/dom-view-transitions": "^1.0.4",
		"bootstrap-icons": "^1.11.3",
		"compare-versions": "^6.1.0",
		"eslint-plugin-svelte": "^2.37.0",
		"front-matter": "^4.0.2",
		"prettier-plugin-svelte": "^3.2.3",
		"shiki": "^1.3.0",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.9",
		"svelte-markdown": "^0.4.1",
		"tsx": "^4.7.2"
	}
}
