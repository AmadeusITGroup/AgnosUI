{
	"name": "@agnos-ui/demo",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "wireit",
		"dev:stackblitz": "wireit",
		"dev:coverage": "wireit dev",
		"build": "wireit",
		"generate:doc": "wireit",
		"build:coverage": "wireit",
		"preview": "wireit",
		"preview:coverage": "wireit",
		"check": "wireit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"tdd": "vitest",
		"tdd:ui": "vitest --ui",
		"update:stackblitzPkgs": "node scripts/updateStackblitzPkgs.js"
	},
	"wireit": {
		"generate:doc": {
			"command": "tsx --tsconfig ../scripts/doc/tsconfig.json ../scripts/doc/generate-docs.ts",
			"files": [
				"../core/src/**",
				"../core-bootstrap/src/**",
				"../scripts/doc/tsconfig.json",
				"../scripts/doc/generate-docs.ts",
				"../scripts/doc/api-doc.ts"
			],
			"output": [
				"generated/**"
			]
		},
		"sveltekit": {
			"command": "svelte-kit sync",
			"dependencies": [
				"../svelte/preprocess:build"
			],
			"files": [
				"svelte.config.js",
				"src/**"
			],
			"output": [
				".svelte-kit/**",
				"!.svelte-kit/{generated,output}/**"
			]
		},
		"dev": {
			"command": "vite dev",
			"dependencies": [
				"generate:doc"
			],
			"service": true
		},
		"dev:stackblitz": {
			"command": "vite dev",
			"dependencies": [
				"generate:doc",
				"../angular/bootstrap:build",
				"../react/bootstrap:build",
				"../svelte/bootstrap:build"
			],
			"service": true,
			"env": {
				"STACKBLITZ": "true"
			}
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"generate:doc",
				"../angular/demo:build",
				"../react/demo:build",
				"../svelte/demo:build",
				"sveltekit"
			],
			"env": {
				"STACKBLITZ": "true"
			},
			"files": [
				"static/**",
				"vite.config.ts",
				"vite-env.d.ts",
				"scripts/*"
			],
			"output": [
				"dist/**",
				".svelte-kit/{generated,output}/**"
			]
		},
		"build:coverage": {
			"command": "vite build",
			"dependencies": [
				"generate:doc",
				"../angular/demo:build:coverage",
				"../react/demo:build:coverage",
				"../svelte/demo:build:coverage",
				"../svelte/bootstrap:build",
				"sveltekit"
			],
			"files": [
				"static/**",
				"vite.config.ts",
				"vite-env.d.ts",
				"scripts/*"
			],
			"output": [
				"dist/**",
				".svelte-kit/{generated,output}/**"
			]
		},
		"preview": {
			"command": "vite preview",
			"dependencies": [
				"build"
			],
			"service": true
		},
		"preview:coverage": {
			"command": "vite preview",
			"dependencies": [
				"build:coverage"
			],
			"service": true
		},
		"check": {
			"command": "svelte-check",
			"dependencies": [
				"build"
			],
			"files": [
				"src/**",
				"tsconfig.json"
			],
			"output": []
		}
	},
	"dependencies": {
		"@agnos-ui/svelte-bootstrap": "0.0.0",
		"@amadeus-it-group/tansu": "^1.0.0",
		"@floating-ui/dom": "^1.6.5"
	},
	"devDependencies": {
		"@stackblitz/sdk": "^1.10.0",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.16",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/dom-view-transitions": "^1.0.4",
		"bootstrap-icons": "^1.11.3",
		"compare-versions": "^6.1.0",
		"eslint-plugin-svelte": "^2.39.4",
		"front-matter": "^4.0.2",
		"prettier-plugin-svelte": "^3.2.4",
		"shiki": "^1.6.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.0",
		"svelte-markdown": "^0.4.1",
		"tsx": "^4.15.5"
	}
}
