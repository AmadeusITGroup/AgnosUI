{
	"compilerOptions": {
		"jsx": "react-jsx",
		"baseUrl": "./",
		"target": "es2022",
		"module": "esnext",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true
	}
}
