{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"mainEntryPointFilePath": "<projectFolder>/dist/dts/index.d.ts",
	"docModel": {
		"enabled": false
	},
	"dtsRollup": {
		"enabled": true,
		"untrimmedFilePath": "<projectFolder>/dist/lib/index.d.ts"
	},
	"apiReport": {
		"enabled": false
	},
	"messages": {
		"extractorMessageReporting": {
			"ae-missing-release-tag": {
				"logLevel": "none"
			}
		}
	},
	"compiler": {
		"tsconfigFilePath": "<projectFolder>/tsconfig.d.json"
	},
	"tsdocMetadata": {
		"enabled": false
	}
}
